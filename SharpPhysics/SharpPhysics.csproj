<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Title>SharpPhysics 2d Physics Simulator and Renderer</Title>
    <Company />
    <Authors />
    <Version>1.4.3</Version>
    <RepositoryUrl>https://github.com/TheVave/SharpPhysics</RepositoryUrl>
    <AnalysisLevel>6.0-all</AnalysisLevel>
    <GenerateDocumentationFile>False</GenerateDocumentationFile>
    <AssemblyName>SharpPhysics</AssemblyName>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <WarningLevel>1</WarningLevel>
    <DebugType>embedded</DebugType>
    <FileAlignment>1024</FileAlignment>
    <IsTrimmable>True</IsTrimmable>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <WarningLevel>0</WarningLevel>
    <DebugType>embedded</DebugType>
    <FileAlignment>1024</FileAlignment>
    <IsTrimmable>True</IsTrimmable>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="deps\**" />
    <EmbeddedResource Remove="deps\**" />
    <None Remove="deps\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="2d\Physics\CollisionManagment\CollisionManagerHelper.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Cursor1.cur" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Docs.txt">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="2d\ObjectRepresentation\PhysicsPrams.cs">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Compile>
    <Compile Update="2d\ObjectRepresentation\Point.cs">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
