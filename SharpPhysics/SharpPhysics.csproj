    </Compile>
    <Compile Update="2d\Objects\_2dBaseObjects.cs">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Compile>
    <Compile Update="2d\Physics\2dPhysicsSimulator.cs">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Compile>
    <Compile Update="2d\Physics\CollisionManagment\CollisionManager.cs">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Compile>
    <Compile Update="2d\Physics\MovementRepresenter.cs">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Compile>
    <Compile Update="2d\Raycasting\_2dLine.cs">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Compile>
